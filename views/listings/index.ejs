<%  layout("/layout/boilerplate")%>
  <body>
    <h3>All Listings</h3>
    <form action="/listings/new" method="GET">
      <button>Create New Listing</button>
    </form>
    <ul>
      <% for(listing of allListings){%>
      <li>
        <a href="/listings/<%=listing._id %>"> <%= listing.title %></a> <%}%>
      </li>
    </ul>
  </body>

